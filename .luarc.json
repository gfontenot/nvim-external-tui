{
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "globals": [
      "vim",
      "describe",
      "it",
      "before_each",
      "after_each",
      "assert"
    ],
    "disable": [
      "undefined-field"
    ]
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME/lua"
    ],
    "checkThirdParty": false
  }
}
